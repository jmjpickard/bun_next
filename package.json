{
  "name": "bun_next",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch run dev.tsx",
    "tailwind:watch": "npx tailwindcss -i ./src/input.css -o ./build/output.css --watch",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./build/output.css",
    "build": "npm run tailwind && bun run dev.tsx"
  },
  "devDependencies": {
    "bun-types": "latest",
    "tailwindcss": "^3.3.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
